{
  "name": "cccat_16_node",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "rimraf dist && tsc -p tsconfig-build.json -w",
    "up": "npm run build && docker compose up -d --build",
    "down": "docker compose down",
    "logs": "docker compose logs app -f",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watchAll -c jest-unit-config.js",
    "test:integration": "npm test -- --watchAll -c jest-integration-config.js",
    "test:request": "npm test -- --watchAll -c jest-request-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix && eslint tests/**/*.ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/errorhandler": "^1.5.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.10",
    "dotenv": "^16.4.5",
    "errorhandler": "^1.5.1",
    "express": "^4.19.2",
    "git-commit-msg-linter": "^5.0.7",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.0",
    "pg-promise": "^11.6.0",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@types/faker": "^5.5.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "eslint": "^8.0.1",
    "eslint-config-love": "^47.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "faker": "^5.5.3",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": "20.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
